{
  "name": "YouTube Live Comments",
  "description": "Allows you to view comments left for specific timestamps",
  "version": "0.1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["jquery-3.6.0.min.js", "background.js"]
  },
  "icons": {
    "16": "icon/icon16.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  },
  "browser_action": {
    "default_title": "YouTube Live Comments"
  },
  "permissions": ["tabs", "activeTab", "storage", "https://www.youtube.com/*"]
}
